    <body>
        <form id="form sign-up">
        <div class="vh-100 d-flex justify-content-center align-items-center ">
            <div class="col-md-4 p-5 shadow-sm border rounded-5 border-success ">
                <h2 class="text-center mb-4 text-success">Sign Up</h2>
                <form id="signup-form">
                    <div class="mb-3">
                        <label for="reg-user" class="form-label">Username:</label>
                        <input type="text" class="form-control border border-success" id="name-signup" aria-describedby="regUser1">
                    </div>
                    <div class="mb-3">
                        <label for="reg-email" class="form-label">Email:</label>
                        <input type="email" class="form-control border border-success" id="email-signup">
                    </div>
                    <div class="mb-3">
                        <label for="reg-password" class="form-label">Password:</label>
                        <input type="password" class="form-control border border-success" id="password-signup">
                    </div>
                    <div class="d-grid">
                        <label for="submit" button class="btn btn-success" type="submit">
                            <button id="signup-form">Sign Up</button>
                    </div>
                </form>
                <div class="mt-3">
                    <p class="mb-0  text-center">Already a user?<a href="/logins"
                            class="text-success fw-bold">Sign in</a></p>
                </div>
            </div>
        </div>
